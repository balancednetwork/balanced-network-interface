{
  "name": "balanced-network-stats",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@balancednetwork/balanced-js": "workspace:*",
    "@balancednetwork/sdk-core": "workspace:*",
    "@popperjs/core": "2.8.4",
    "@reach/dialog": "0.18.0",
    "@reach/portal": "0.18.0",
    "@tanstack/react-query": "5.29.2",
    "axios": "0.21.3",
    "bignumber.js": "9.0.1",
    "dayjs": "1.10.7",
    "icon-sdk-js": "1.5.1",
    "lightweight-charts": "3.2.0",
    "numbro": "^2.5.0",
    "polished": "4.0.5",
    "react": "^18.2.0",
    "react-click-away-listener": "2.2.3",
    "react-datepicker": "4.2.1",
    "react-device-detect": "2.2.3",
    "react-dom": "^18.2.0",
    "react-helmet-async": "1.3.0",
    "react-is": "18.2.0",
    "react-popper": "2.3.0",
    "react-router-dom": "6",
    "react-use": "17.5.0",
    "rebass": "4.0.7",
    "recharts": "2.10.3",
    "sanitize.css": "12.0.1",
    "styled-components": "6.1.8"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "checkTs": "tsc --noEmit",
    "lint:css": "stylelint src/**/*.css",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf tsconfig.tsbuildinfo"
  },
  "proxy": "https://balanced.sudoblock.io",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=18.0.0",
    "yarn": "^1.22.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "@babel/runtime": "^7.25.0",
    "@types/lodash": "4.14.168",
    "@types/node": "^22.1.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "6.1.6",
    "@types/react-router-dom": "^5",
    "@types/rebass": "4.0.7",
    "@types/styled-components": "5.1.3",
    "@vitejs/plugin-react": "4.2.1",
    "babel-runtime": "6.26.0",
    "husky": "4.2.5",
    "lint-staged": "10.3.0",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "36.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.3.5",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.3.1"
  }
}
