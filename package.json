{
  "name": "balanced-network-interface",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "workspaces": ["apps/*", "packages/*"],
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "turbo run dev --filter=web...",
    "dev:stats": "turbo run dev --filter=stats...",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" turbo run build --filter=web...",
    "build:stats": "turbo run build --filter=stats...",
    "pretty": "turbo run pretty",
    "clean": "turbo run clean && rm -rf node_modules && rm -rf .turbo",
    "checkTs": "turbo run checkTs",
    "lint": "turbo run lint",
    "prepare": "husky"
  },
  "pnpm": {
    "overrides": {
      "@sodax/sdk": "0.0.1-rc.42",
      "@sodax/dapp-kit": "0.0.1-rc.29",
      "@sodax/wallet-sdk-core": "0.0.1-rc.2",
      "@sodax/types": "0.0.1-rc.17",
      "wagmi": "2.14.12",
      "viem": "2.29.2"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "turbo": "^2.0.12",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "neverthrow": "^8.2.0"
  }
}
