{
  "name": "@balancednetwork/v1-sdk",
  "license": "MIT",
  "version": "1.1.2",
  "description": "ðŸ›  An SDK for building applications on top of BalancedNetwork V1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": ["dist", "src"],
  "repository": "https://github.com/balancednetwork/v1-sdk.git",
  "scripts": {
    "lint": "tsup lint src",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "tsup test",
    "prepublishOnly": "tsup",
    "clean": "rm -rf dist && rm -rf node_modules",
    "pretty": "biome format . --write",
    "checkTs": "tsc --noEmit"
  },
  "dependencies": {
    "@balancednetwork/sdk-core": "workspace:*",
    "@ethersproject/address": "^5.0.0",
    "@ethersproject/solidity": "^5.0.0",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "tsup": "^8.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
